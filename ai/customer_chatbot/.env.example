# Customer Chatbot Environment Configuration
# Copy this file to .env and fill in the values

# LLM Generator settings
GENERATOR_TYPE=gemini # options: gemini, cohere

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Cohere API
COHERE_API_KEY=your_cohere_api_key_here

# Backend API
BACKEND_API_URL=http://localhost:3001

# Database (PostgreSQL with pgvector)
DATABASE_HOST=localhost
DATABASE_PORT=5433
DATABASE_USER=admin
DATABASE_PASSWORD=password
DATABASE_NAME=real_estate_crm

# Embedding Model
EMBEDDING_MODEL_NAME=mohamed2811/Muffakir_Embedding_V2

# WhatsApp API (optional, for production)
WHATSAPP_VERIFY_TOKEN=your_verify_token
WHATSAPP_ACCESS_TOKEN=your_access_token
